package com.hcl.java;

public class Prime {
	public void show(int n){
		int i=1;
		while(i<n/2){
			if(n%i==0)
			{
				flag=0;
			}
			i++;
		}
		if(flag==0)
		{
			System.out.println("prime");
		}
		else
		{
			System.out.println("not prime");
		}
	}
		else{
			System.out.println("non prime");
		}
public static void main(String[] args) {
	int n;
	n=2;
	new Prime().show(n);
	
}
}
