package com.hcl.collections;
interface IHello{
	void sayHello();
}
public class Lambda2 {
public static void main(String[] args) {
	IHello h1=()->{
		System.out.println("hello from sri..");
	};
	IHello h2=()->{
		System.out.println("hello from nivi");
	};
	IHello h3=()->{
		System.out.println("hello from visha");
	};
	h1.sayHello();
	h2.sayHello();
	h3.sayHello();
}
}
