package com.hcl.collections;
class Data{
	public void swap(int x,int y){
		int t;
		t=x;
		x=y;
		y=t;
		System.out.println("x value "+x+"y value "+y);
	}
}
public class SwapDemo {
	public static void main(String[] args) {
		Data obj=new Data();
		int x,y;
		x=12;
		y=13;
		obj.swap(x, y);
		double b1=10.5,b2=33.5;
		obj.swap(b1, b2);
	}
}
