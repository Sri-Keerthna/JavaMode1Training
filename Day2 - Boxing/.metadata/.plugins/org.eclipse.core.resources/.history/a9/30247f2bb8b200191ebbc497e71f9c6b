package com.hcl.inh;

	class First{
		public First(){
			System.out.println("base class method");
		}
	}
	
	class Second extends First{
		public Second(){
			System.out.println("derived class method");
		}
	}
	
	public class Quiz2 {
		public static void main(String[] args) {
			new Second();
		}
}

