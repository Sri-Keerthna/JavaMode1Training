package com.hcl.inh;
class A {
	public0. int x=10;
	 void show(){
		System.out.println("xxx");
	}
	
}
class B extends A{
	protected void display(){
		System.out.println("yyy");
	}
	private void shows(int x){
		System.out.println("value is "+x);
	}
	void dis(){
		System.out.println("vfv");
	}
}
class AccessModif
{
	public static void main(String[] args) {
	B b=new B();
	b.display();
	b.dis();
	b.show();
	}
}
