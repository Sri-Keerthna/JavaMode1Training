package com.hcl.ex;

public class ArrEx {
public static void main(String[] args) {
	int[] a=new int[]{12,5};
	try {
		a[10]=77;
	} 
	catch (ArrayIndexOutOfBoundsException e) {
		// TODO: handle exception
		System.out.println("array size should not be exceeded");
	}
	catch (Exception e) {
		// TODO Auto-generated catch block
		e.printStackTrace();
	}
}
}
