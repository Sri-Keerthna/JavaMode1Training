package com.hcl.ex;
class First{
	public void show(){
		System.out.println("show method from first");
	}
}
class Second{
	public void show(){
		System.out.println("show method from second");
	}
}
public class Quiz1 {
public static void main(String[] args) {
	First ob=new First();
	Second ob2=(Second)ob;
	ob2.show();
}
}
